{
  "name": "react-template",
  "version": "1.0.0",
  "private": true,
  "description": "React template project.",
  "homepage": "",
  "author": "Jimmy Liikala",
  "license": "MIT",
  "main": "src/index.tsx",
  "bugs": {
    "url": "https://github.com/jimli570/react-template/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jimli570/react-template.git"
  },
  "keywords": [
    "React",
    "Template"
  ],
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-scripts test --watchAll=false",
    "test:watch": "react-scripts test",
    "test:coverage": "npm run test -- --coverage",
    "test:dumpcoverage": "npm run test:coverage > out/coverage.out",
    "lint": "eslint src/",
    "lint:dumpcfg": "eslint --print-config lintcfg.out > out/lint.out",
    "husky:setup": "husky install && npm run husky:setup",
    "husky:makeexecutable": "sh scripts/setupHusky.sh",
    "eject": "react-scripts eject",
    "upload-to-server": "sh scripts/uploadToServer.sh"
  },
  "dependencies": {
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-redux": "^8.0.1",
    "react-router-dom": "^6.3.0",
    "@apollo/client": "^3.6.1"
  },
  "devDependencies": {
    "@reduxjs/toolkit": "^1.8.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.1.1",
    "@testing-library/user-event": "^14.1.1",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.30",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "@types/react-redux": "^7.1.24",
    "@types/react-router-dom": "^5.3.3",
    "customize-cra": "^1.0.0",
    "husky": "^7.0.4",
    "react-app-rewired": "^2.2.1",
    "react-scripts": "5.0.1",
    "typescript": "^4.6.4",
    "web-vitals": "^2.1.4"
  },
  "optionalDependencies ": {
    "create-react-app": "^5.0.1",
    "cra-template-redux-typescript": "2.0.0",
    "react-bootstrap": "^2.3.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
